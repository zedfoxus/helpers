filetype plugin indent on
set number
syntax on
set tabstop=4		" Width of \t; just that VIM will interpret it to width 4
set shiftwidth=4	" Indents will have a width of 4
set noexpandtab		" Does NOT expand tab to spaces
set softtabstop=4   " Sets the number of columns for a TAB.
set autoindent
set nocompatible	" ConEmu MinGW related
set backspace=2		" ConEmu MinGW related
